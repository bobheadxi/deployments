{
  "name": "@maxgfr/deployments",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action for working painlessly with deployment statuses",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier src --write",
    "build": "ncc build src/main.ts --out dist --minify --source-map --license LICENSES"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxgfr/deployments.git"
  },
  "author": "maxgfr",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0",
    "@vercel/ncc": "0.28.6"
  },
  "devDependencies": {
    "@types/node": "12.20.15",
    "husky": "^7.0.4",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.2",
    "typescript": "4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
